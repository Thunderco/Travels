         url('../fonts/canter/canter_bold-webfont.ttf') format('truetype'),
         url('../fonts/canter/canter_bold-webfont.svg#canterbold') format('svg');
    font-weight: normal;
    font-style: normal;
}

@import 'nib/vendor'
vendor-prefixes = o ms moz webkit official

naranja = #f9840d
naranjaf = #d56100
naranjat = #632d00
azul_texto = #155480

html, body 
	height 100%
	line-height 1.5
	font-size 18px

h1,h2,h3,h4,h5,h6,p,span{font-family:inherit;font-size:inherit;}

.clear
	clear both

a
	transition all .5s ease-in-out

.hidden{
     opacity:0;
}
.visible{
     opacity:1;
}

